<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net6.0-windows</TargetFramework>
        <UseWPF>true</UseWPF>
        <ApplicationIcon>..\${relProj}\SpacePi.Dashboard.WPFMain\Images\Icon.ico</ApplicationIcon>
        <EnableDefaultItems>false</EnableDefaultItems>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="SpacePi.Dashboard.API" Version="3.0.0" />
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="..\${relProj}\SpacePi.Dashboard.Analyzer\SpacePi.Dashboard.Analyzer.csproj" OutputItemType="Analyzer" ReferenceOutputAssembly="false" />
        <ProjectReference Include="..\${relProj}\SpacePi.Dashboard.WPF\SpacePi.Dashboard.WPF.csproj" />
$<TARGET_PROPERTY:dashboard-metadata,SPACEPI_DASHBOARD_CSPROJ_REFS>    </ItemGroup>
    <ItemGroup>
        <Content Include="..\${relProj}\SpacePi.Dashboard.WPFMain\Images\Icon.ico" Link="Images\Icon.ico" />
    </ItemGroup>
    <ItemGroup>
        <Compile Include="..\${relProj}\SpacePi.Dashboard.WPFMain\Entry.cs" Link="Entry.cs" />
    </ItemGroup>
</Project>
